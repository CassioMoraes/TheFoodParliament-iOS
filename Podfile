# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'TheFoodParlament' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for TheFoodParlament
  pod 'GoogleMaps'
  pod 'Alamofire'
  pod 'SwiftyJSON'
  pod 'Firebase/Core'
  pod 'Firebase/Database'

  target 'TheFoodParlamentTests' do
    inherit! :search_paths
    pod 'Firebase'
    # Pods for testing
  end

  target 'TheFoodParlamentUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
